{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  }
}
